			<s:stroke>
				<mx:SolidColorStroke  weight="1"  color="0" />
			</s:stroke>
		</s:Line>
		<s:Line right="0" left="0"   >
			<s:stroke>
				<mx:SolidColorStroke  weight="1"  color="0x666666" />
			</s:stroke>
		</s:Line>	
	</s:VGroup>
 
	 
 
</s:Skin>
